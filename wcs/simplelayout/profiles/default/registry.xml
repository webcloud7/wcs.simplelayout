<registry xmlns:i18n="http://xml.zope.org/namespaces/i18n"
          i18n:domain="plone">
    <record name="plone.displayed_types">
        <value purge="false">
            <element>ContentPage</element>
            <element>NewsFolder</element>
        </value>
    </record>
    <record name="plone.default_page_types">
        <value purge="false">
            <element>ContentPage</element>
        </value>
    </record>
    <records prefix="plone.bundles/simplelayout" interface='Products.CMFPlone.interfaces.IBundleRegistry'>
        <value key="enabled">True</value>
        <value key="expression">python: member is not None</value>
        <value key="jscompilation">++resource++simplelayout/js/dist/simplelayout-remote.min.js</value>
        <value key="csscompilation"></value>
        <value key="load_async">False</value>
        <value key="load_defer">False</value>
        <value key="depends">bootstrap-js</value>
    </records>
</registry>
